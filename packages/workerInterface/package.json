{
  "name": "@mindburn/workerInterface",
  "version": "0.1.0",
  "description": "Telegram Bot interface for Aletheia workers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:prod": "serverless deploy --stage prod",
    "logs:dev": "serverless logs --stage dev --tail",
    "logs:staging": "serverless logs --stage staging --tail",
    "logs:prod": "serverless logs --stage prod --tail"
  },
  "dependencies": {
    "@tonconnect/sdk": "^3.0.0",
    "telegraf": "^4.16.3",
    "telegraf-session-dynamodb": "^1.3.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1550.0",
    "aws-xray-sdk-core": "^3.5.3",
    "@mindburn/shared": "workspace:*",
    "@aws-sdk/client-dynamodb": "^3.529.1",
    "@aws-sdk/client-lambda": "^3.529.1",
    "@aws-sdk/client-sqs": "^3.529.1",
    "@aws-sdk/lib-dynamodb": "^3.529.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "serverless-webpack": "^5.13.0",
    "serverless-offline": "^13.3.3",
    "serverless-prune-plugin": "^2.0.2",
    "webpack": "^5.90.1"
  }
}