# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here
WEBHOOK_DOMAIN=your_webhook_domain.com
WEBHOOK_PATH=/webhook/telegram/worker-bot

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# DynamoDB Configuration
DYNAMODB_TABLE_PREFIX=mindburn_worker_bot_
DYNAMODB_SESSIONS_TABLE=sessions
DYNAMODB_USERS_TABLE=users
DYNAMODB_TASKS_TABLE=tasks

# TON Configuration
TON_NETWORK=testnet
TON_ENDPOINT=https://toncenter.com/api/v2/jsonRPC
TON_API_KEY=your_api_key_here

# API Configuration
API_URL=http://localhost:3000/api
API_KEY=your_api_key_here

# Feature Flags
ENABLE_TESTNET=true
ENABLE_ANALYTICS=true

# Logging
LOG_LEVEL=info 